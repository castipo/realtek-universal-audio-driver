AddComponent = DolbyAPO_DAX3HSA_DolbyAtmos,, DolbyAPO_ComponentInstall_DAX3HSA_DolbyAtmos

[DeviceExtension_Install_DolbyAtmosSpeakerSystem.NT.Components]
